{
    "env": {
        "node": true,
        "es2020": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
      "space-before-function-paren": "off",
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": ["error"]
    }
}
